{
    # Just the headlight.
    "basic": {
        "breath": "gantom.dimmer",
        "note": "gantom.color_macro"
    },

    # Same, but restrict the dimmer to max 192.
    "a_bit_less_bright": {
        "breath": {
            "light": "gantom.dimmer",
            "range": {"end": 192}
        },
        "note": "gantom.color_macro"
    },

    # Controls two lights.
    "big": {
        "breath": [
            "gantom.dimmer",
            "moving.dimmer"
        ]
    }
}
